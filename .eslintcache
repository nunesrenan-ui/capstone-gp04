ommet,\n  Box,\n  Form,\n  FormField,\n  TextInput,\n  MaskedInput,\n  Text,\n  Button,\n  Heading,\n} from \"grommet\";\nimport { dark } from \"grommet/themes\";\nimport {\n  MailOption,\n  User,\n  Phone,\n  Hide,\n  View,\n  Lock,\n  StatusGood,\n} from \"grommet-icons\";\n\nimport { useState } from \"react\";\n\nimport { useHistory } from \"react-router-dom\";\n\nimport axios from \"axios\";\n\nconst requi = \"*\";\nconst UserRegister = (props) => {\n  const history = useHistory();\n  const [nameVal, setNameVal] = useState(false);\n  const [lastNameVal, setLastNameVal] = useState(false);\n  const [phoneVal, setPhoneVal] = useState(false);\n  const [emailVal, setEmailVal] = useState(false);\n  const [passwordVal, setPasswordVal] = useState(false);\n  const [confirmPasswordVal, setConfirmPasswordVal] = useState(false);\n\n  const [value, setValue] = useState({\n    name: \"\",\n    lastName: \"\",\n    phone: \"\",\n    email: \"\",\n    password: \"\",\n    confirmPassword: \"\",\n  });\n\n  const [reveal, setReveal] = useState(false);\n\n  const onFinish = (values) => {\n    axios\n      .post(\"https://api-capstone-grupo04.herokuapp.com/users\", { ...values })\n      .then((res) => {\n        console.log(res);\n        history.push(\"/login\");\n      })\n      .catch((err) => console.log(err));\n  };\n\n  return (\n    <motion.div\n      initial={{ opacity: 0 }}\n      animate={{ opacity: 1 }}\n      exit={{ opacity: 0 }}\n      transition={{ duration: 1 }}\n    >\n      <Box round background=\"rgba(0, 0, 0, 0.7)\">\n        {/* <Box\n        background=\"#FFC15E\"\n        justify=\"center\"\n        align=\"center\"\n        round\n        pad={{ horizontal: \"xsmall\", vertical: \"xsmall\" }}\n      >\n        <h2>Registre-se</h2>\n      </Box> */}\n        <Header>\n          <div>\n            <Link to=\"/login\">Já sou cadastrado</Link>\n          </div>\n        </Header>\n        <Box\n          background=\"rgba(0, 0, 0, 0)\"\n          align=\"center\"\n          justify=\"center\"\n          pad=\"xsmall\"\n        >\n          <Form\n            value={value}\n            validate=\"blur\"\n            onChange={(val) => setValue(val)}\n            onSubmit={({ value: val }) => onFinish(val)}\n          >\n            <Box direction=\"row\" pad=\"medium\" margin={{ left: \"large\" }}>\n              <FormField\n                label=\"Nome\"\n                name=\"name\"\n                icon={<User />}\n                component={TextInput}\n                required=\"*\"\n                validate={[\n                  { regexp: /^[a-zA-Z]+$/i, message: \"Somente letras.\" },\n                  (name) => {\n                    if (name.length >= 3) {\n                      setNameVal(true);\n                    }\n                    return undefined;\n                  },\n                ]}\n              />\n              <Box align=\"center\" justify=\"center\">\n                {nameVal && <StatusGood />}\n              </Box>\n            </Box>\n\n            <Box direction=\"row\" pad=\"medium\" margin={{ left: \"large\" }}>\n              <FormField\n                label=\"Sobrenome\"\n                name=\"lastName\"\n                icon={<User />}\n                validate={[\n                  { regexp: /^[a-zA-Z ]+$/i, message: \"Somente letras.\" },\n                  (lastName) => {\n                    if (lastName.length >= 2) {\n                      setLastNameVal(true);\n                    }\n                    return undefined;\n                  },\n                ]}\n              />\n              <Box align=\"center\" justify=\"center\">\n                {lastNameVal && <StatusGood />}\n              </Box>\n            </Box>\n\n            <Box direction=\"row\" pad=\"medium\" margin={{ left: \"large\" }}>\n              <FormField\n                label=\"Telefone\"\n                name=\"phone\"\n                icon={<Phone />}\n                validate={[\n                  {\n                    regexp: /^[0-9]{10}$/i,\n                    message: \"Somente números, dez dígitos\",\n                  },\n                  (phone) => {\n                    if (phone.length === 10) {\n                      setPhoneVal(true);\n                    }\n                    return undefined;\n                  },\n                ]}\n              />\n              <Box align=\"center\" justify=\"center\">\n                {phoneVal && <StatusGood />}\n              </Box>\n            </Box>\n\n            <Box direction=\"row\" pad=\"medium\" margin={{ left: \"large\" }}>\n              <FormField\n                label=\"E-mail\"\n                name=\"email\"\n                icon={<MailOption />}\n                required\n                validate={[\n                  {\n                    regexp: /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))+$/,\n                    placeholder: \"exemplo@ex.com\",\n                    message: \"E-mail inválido.\",\n                  },\n                  (email) => {\n                    if (email.length > 5) {\n                      setEmailVal(true);\n                    }\n                    return undefined;\n                  },\n                ]}\n              />\n              <Box align=\"center\" justify=\"center\">\n                {emailVal && <StatusGood />}\n              </Box>\n            </Box>\n\n            <Box direction=\"row\" pad=\"medium\" margin={{ left: \"large\" }}>\n              <FormField\n                label=\"Senha\"\n                name=\"password\"\n                required\n                icon={<Lock />}\n                type={reveal ? \"text\" : \"password\"}\n                validate={[\n                  (password) => {\n                    if (password.length > 2) {\n                      setPasswordVal(true);\n                    }\n                    return undefined;\n                  },\n                ]}\n              />\n              <Button\n                icon={reveal ? <View size=\"medium\" /> : <Hide size=\"medium\" />}\n                onClick={() => setReveal(!reveal)}\n              />\n              <Box align=\"center\" justify=\"center\">\n                {passwordVal && <StatusGood />}\n              </Box>\n            </Box>\n            <Box direction=\"row\" pad=\"medium\" margin={{ left: \"large\" }}>\n              <FormField\n                label=\"Confirmar Senha\"\n                name=\"confirmPassword\"\n                required\n                icon={<Lock />}\n                type={reveal ? \"text\" : \"password\"}\n                validate={[\n                  (confirmPassword) => {\n                    if (confirmPassword === value.password) {\n                      setConfirmPasswordVal(true);\n                      return;\n                    }\n                    return { message: \"As senhas não estão iguais!\" };\n                  },\n                ]}\n              />\n              <Box align=\"center\" justify=\"center\">\n                {confirmPasswordVal && <StatusGood />}\n              </Box>\n            </Box>\n            <Box align=\"center\" pad=\"small\">\n              <Button primary label=\"Enviar\" type=\"submit\" />\n            </Box>\n          </Form>\n        </Box>\n      </Box>\n      <Footer />\n    </motion.div>\n  );\n};\n\nexport default UserRegister;\n\nexport const Header = styled.div`\n  width: 100%;\n  height: 5vh;\n  background-color: #ff9f1c;\n  display: flex;\n  justify-content: flex-end;\n  align-items: center;\n\n  div {\n    margin: 0 1%;\n  }\n`;\n","/home/lucas/Documents/Kenzie/React/capstone-grupo04/src/pages/Home/index.jsx",["143"],"import HomePageCard from \"../../components/HomePageCard/\";\nimport MenuBar from \"../../components/MenuBar/\";\nimport { HomePageLayout, ScrollDownHome } from \"./styles\";\nimport styled from \"styled-components\";\nimport { Link } from \"react-router-dom\";\nimport Footer from \"../../components/Footer\";\nimport { motion } from \"framer-motion\";\n\nconst HomePage = () => {\n  return (\n    <motion.div\n      initial={{ opacity: 0 }}\n      animate={{ opacity: 1 }}\n      exit={{ opacity: 0 }}\n      transition={{ duration: 1 }}\n    >\n      <Header>\n        {/* AQUI VAI TERNARIO PARA SE ESTIVER LOGADO */}\n        <div>\n          <Link to=\"/cadastro\">Seja um doador</Link>\n        </div>\n        <div>\n          <Link to=\"/login\">Login</Link>\n        </div>\n      </Header>\n      <HomePageLayout>\n        <HomePageCard />\n      </HomePageLayout>\n      <ScrollDownHome>\n        <div id=\"section2\">\n          <div className=\"opacity\">\n            <h1>Sejam muito bem-vindos ao Giver!</h1>\n          </div>\n          <div className=\"opacity\">\n            <p>\n              Nossa plataforma foi desenvolvida com muito carinho pelos seus\n              idealizadores para facilitar a vida de nossos usuários.\n            </p>\n            <p>\n              Mas você sabe o que é e como funciona o Giver? Se a resposta for\n              não, trouxemos aqui respostas para as perguntas que recebemos\n              frequentemente para contar um pouquinho da história desta\n              plataforma, bem como sua utilidade. Pois bem, vamos lá!\n            </p>\n          </div>\n          <div className=\"opacity\">\n            <h3>- Para que serve o Giver ?</h3>\n            <p>\n              Primeiramente, fazemos uma pergunta, quem nunca teve aquela\n              mesinha de canto antiga que não tinha mais utilidade em casa e não\n              sabia o que fazer com ela ou aquela peça de roupa que já não serve\n              mais, mas está boa demais para jogar fora?\n            </p>\n            <p>\n              Então, pensando nisso, nossos idealizadores se juntaram para\n              resolver esse problema e a solução foi nosso querido Giver, que\n              conecta pessoas que tem itens para serem doados e pessoas que\n              almejam receber a doação.\n            </p>\n          </div>\n          <div className=\"opacity\">\n            <h3>- E como ele funciona?</h3>\n            <p>\n              Para todos que pretendem usar a plataforma, é necessário realizar\n              um cadastro prévio de usuário. O Cadastro é rapidinho e já te\n              permite acessar a plataforma e seus benefícios.\n            </p>\n            <p>\n              Feito o cadastro, se caso for donatário, você será redirecionado\n              para sua home, onde poderá começar se cadastrar seus interesses. E\n              se caso for doador, precisará então cadastrar o item e, assim, já\n              estará lançando sua doação e possibilitando que os usuários\n              interessados possam cadastrar seu interesse.\n            </p>\n            <p>\n              Encerrado o período de cadastramento de interesse, haverá um\n              sorteio que definirá quem será o donatário sortudo escolhido.\n            </p>\n          </div>\n          <div className=\"opacity\">\n            <h3>- Mas, afinal, quem são o Doador e o Donatário?</h3>\n            <p>\n              Doador é o usuário que se cadastra para doar um item e Donatário é\n              aquele que se cadastra com a intenção de receber essa doação.\n            </p>\n          </div>\n          <div className=\"opacity\">\n            <h3>- O que é o sistema de Score?</h3>\n            <p>\n              Para cada transação feita (doação ou recebimento), o Giver ganhará\n              pontos em seu Score. Para cada tipo de item é nessário um score\n              mínimo, ou seja, para receber também é necessário doar!\n            </p>\n          </div>\n          <div className=\"opacity\">\n            <h3>- Qualquer um pode ser um doador ou um donatário?</h3>\n            <p>\n              Sim senhor! Qualquer pessoa pode se cadastrar e usufruir da\n              plataforma.\n            </p>\n          </div>\n          <div className=\"opacity\">\n            <h3>- Qualquer um pode ser um doador ou um donatário?</h3>\n            <p>\n              Sim senhor! Qualquer pessoa pode se cadastrar e usufruir da\n              plataforma.\n            </p>\n          </div>\n          <div className=\"opacity\">\n            <h3>- Qual o objetivo do Giver?</h3>\n            <p>\n              Nosso objetivo é auxiliar na reutilização das coisas e,\n              consequentemente, realizar integração social, conectando pessoas\n              que, por terem interessesm em comum, podem se ajudar.\n            </p>\n            <p>\n              Gostou e quer saber mais? Então vem com a gente e torne-se um\n              Giver!\n            </p>\n            <br />\n            <p>\n              <a href=\"linktoRegister\">Clique aqui</a> e cadastre-se pra não\n              perder nenhuma oportunidade!\n            </p>\n          </div>\n        </div>\n      </ScrollDownHome>\n\n      <Footer />\n    </motion.div>\n  );\n};\n\nexport default HomePage;\n\nexport const Header = styled.div`\n  width: 100%;\n  height: 5vh;\n  background-color: #ff9f1c;\n  display: flex;\n  justify-content: flex-end;\n  align-items: center;\n\n  div {\n    margin: 0 1%;\n  }\n`;\n","/home/lucas/Documents/Kenzie/React/capstone-grupo04/src/pages/Home/styles.js",[],"/home/lucas/Documents/Kenzie/React/capstone-grupo04/src/components/MenuBar/index.jsx",[],"/home/lucas/Documents/Kenzie/React/capstone-grupo04/src/components/HomePageCard/styles.js",[],["144","145"],"/home/lucas/Documents/Kenzie/React/capstone-grupo04/src/components/DonateTab/index.js",["146","147","148","149","150"],"//ACTIONS\nimport { addDonation } from \"../../store/modules/AddDonation/actions\";\n\n//GROMMET COMPONENTS\nimport {\n  Box,\n  Button,\n  Form,\n  FormField,\n  RadioButtonGroup,\n  Select,\n  T