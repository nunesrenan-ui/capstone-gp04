   model: \"\",\n    author: \"\",\n    title: \"\",\n    description: \"\",\n    date: \"\",\n    frontSide: \"\",\n    backSide: \"\",\n    side: \"\",\n    number: \"\",\n    state: \"\",\n    cep: \"\",\n  });\n\n  const clear = () => {\n    setDonationState(\"\");\n    setBrand(\"\");\n    setSize(\"\");\n    setModel(\"\");\n    setDate(\"\");\n    setDescription(\"\");\n    setfrontSide(\"\");\n    setBackSide(\"\");\n    setSide(\"\");\n    setTitle(\"\");\n    setAuthor(\"\");\n    setState(\"\");\n    setNumber(\"\");\n    setCep(\"\");\n  };\n\n  const checkData = () => {\n    setValue({\n      donation: value.donation,\n      donationState: value.donationState,\n      brand: value.brand,\n      model: value.model,\n      author: value.author,\n      title: value.title,\n      description: value.description,\n      date: value.date,\n      frontSide: value.frontSide,\n      backSide: value.backSide,\n      side: value.side,\n      number: value.number,\n      state: value.state,\n      cep: value.cep,\n      userId: userId,\n    });\n    if (value.donation === \"\") {\n      delete value.donation;\n    }\n    if (value.donationState === \"\") {\n      delete value.donationState;\n    }\n    if (value.brand === \"\") {\n      delete value.brand;\n    }\n    if (value.model === \"\") {\n      delete value.model;\n    }\n    if (value.author === \"\") {\n      delete value.author;\n    }\n    if (value.title === \"\") {\n      delete value.title;\n    }\n    if (value.description === \"\") {\n      delete value.description;\n    }\n  };\n\n  const onSubmit = () => {\n    checkData();\n    axios\n      .post(\"https://api-capstone-grupo04.herokuapp.com/produtos\", value, {\n        headers: { Authorization: `Bearer ${token}` },\n      })\n      .then((res) => {\n        console.log(res);\n        clear();\n      })\n      .catch((err) => console.log(err));\n  };\n\n  return (\n    <Box round background=\"rgba(0, 0, 0, 0.7)\" direction=\"row\" pad=\"medium\">\n      <Form\n        name=\"userId\"\n        onChange={(value) => setValue(value)}\n        onReset={() => {\n          clear();\n        }}\n        onSubmit={onSubmit}\n      >\n        <FormField label=\"Tipo de Doação\" name=\"donation\">\n          <RadioButtonGroup\n            direction=\"row\"\n            name=\"donation\"\n            options={[\"Vestuário\", \"Livros\", \"Eletrônicos\"]}\n            value={donation}\n            onChange={(event) => {\n              setDonation(event.target.value);\n              clear();\n            }}\n          />\n        </FormField>\n\n        {(donation === \"Vestuário\" || donation === \"Eletrônicos\") && (\n          <FormField label=\"Marca\" name=\"brand\">\n            <TextInput\n              name=\"brand\"\n              value={brand}\n              onChange={(event) => setBrand(event.target.value)}\n            />\n          </FormField>\n        )}\n\n        {donation === \"Vestuário\" && (\n          <FormField label=\"Tamanho\" name=\"size\">\n            <Select\n              name=\"size\"\n              options={[\"P\", \"M\", \"G\", \"GG\", \"XGG\"]}\n              value={size}\n              onChange={(event) => setSize(event.option)}\n            />\n          </FormField>\n        )}\n\n        {donation === \"Eletrônicos\" && (\n          <FormField label=\"Modelo\" name=\"model\">\n            <TextInput\n              name=\"model\"\n              value={model}\n              onChange={(event) => setModel(event.target.value)}\n            />\n          </FormField>\n        )}\n\n        {donation === \"Livros\" && (\n          <Box>\n            <FormField label=\"Autor\" name=\"author\">\n              <TextInput\n                name=\"author\"\n                value={author}\n                onChange={(event) => setAuthor(event.target.value)}\n              />\n            </FormField>\n            <FormField label=\"Título\" name=\"title\">\n              <TextInput\n                required\n                name=\"title\"\n                value={title}\n                onChange={(event) => setTitle(event.target.value)}\n              />\n            </FormField>\n          </Box>\n        )}\n\n        <FormField label=\"Estado da Doação\" name=\"donationState\" required>\n          <Select\n            options={[\"Novo\", \"Semi-novo\", \"Usado\"]}\n            name=\"donationState\"\n          />\n        </FormField>\n\n        <FormField label=\"Fotos\">\n          <PhotoContainer direction=\"row\">\n            <TextInput\n              icon={<Camera />}\n              placeholder=\"Frontal\"\n              name=\"frontSide\"\n              value={frontSide}\n              onChange={(event) => setfrontSide(event.target.value)}\n            />\n\n            <TextInput\n              icon={<Camera />}\n              placeholder=\"Traseira\"\n              name=\"backSide\"\n              value={backSide}\n              onChange={(event) => setBackSide(event.target.value)}\n            />\n\n            <TextInput\n              icon={<Camera />}\n              placeholder=\"Lateral\"\n              name=\"side\"\n              value={side}\n              onChange={(event) => setSide(event.target.value)}\n            />\n          </PhotoContainer>\n        </FormField>\n\n        <FormField label=\"Descrição\" name=\"description\">\n          <TextArea\n            name=\"description\"\n            value={description}\n            onChange={(event) => setDescription(event.target.value)}\n          />\n        </FormField>\n\n        <FormField label=\"Data para Sorteio\">\n          <DateInput\n            name=\"date\"\n            value={date}\n            format=\"dd/mm/yyyy\"\n            onChange={(event) => setDate(event.value)}\n          />\n        </FormField>\n\n        <FormField label=\"Local de Entrega\">\n          <Box direction=\"row\" justify=\"between\">\n            <FormField width=\"150px\">\n              <TextInput\