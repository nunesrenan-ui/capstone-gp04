confirmPasswordVal, setConfirmPasswordVal] = useState(false);\n\n  const [value, setValue] = useState({\n    name: \"\",\n    lastName: \"\",\n    phone: \"\",\n    email: \"\",\n    password: \"\",\n    confirmPassword: \"\",\n  });\n\n  const [reveal, setReveal] = useState(false);\n\n  const onFinish = (values) => {\n    axios\n      .post(\"https://api-capstone-grupo04.herokuapp.com/users\", { ...values })\n      .then((res) => {\n        console.log(res);\n        history.push(\"/login\");\n      })\n      .catch((err) => console.log(err));\n  };\n\n  return (\n    <Box round>\n      <CardHeader>\n        <h2>Registre-se</h2>\n      </CardHeader>\n\n      <Box align=\"center\" justify=\"center\" pad=\"large\">\n        <Form\n          value={value}\n          validate=\"blur\"\n          onChange={(val) => setValue(val)}\n          onSubmit={({ value: val }) => onFinish(val)}\n        >\n          <Box direction=\"row\">\n            <FormField\n              label=\"Nome\"\n              name=\"name\"\n              icon={<User />}\n              component={TextInput}\n              required\n              validate={[\n                { regexp: /^[a-zA-Z]+$/i, message: \"Somente letras.\" },\n                (name) => {\n                  if (name.length >= 3) {\n                    setNameVal(true);\n                  }\n                  return undefined;\n                },\n              ]}\n            />\n            <Box align=\"center\" justify=\"center\">\n              {nameVal && <StatusGood />}\n            </Box>\n          </Box>\n\n          <Box direction=\"row\">\n            <FormField\n              label=\"Sobrenome\"\n              name=\"lastName\"\n              icon={<User />}\n              validate={[\n                { regexp: /^[a-zA-Z ]+$/i, message: \"Somente letras.\" },\n                (lastName) => {\n                  if (lastName.length >= 2) {\n                    setLastNameVal(true);\n                  }\n                