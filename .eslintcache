\n} from \"grommet-icons\";\n\nimport { useState } from \"react\";\n\nimport { useHistory } from \"react-router-dom\";\n\nimport axios from \"axios\";\n\nconst UserRegister = () => {\n  const history = useHistory();\n  const [nameVal, setNameVal] = useState(false);\n  const [lastNameVal, setLastNameVal] = useState(false);\n  const [phoneVal, setPhoneVal] = useState(false);\n  const [emailVal, setEmailVal] = useState(false);\n  const [passwordVal, setPasswordVal] = useState(false);\n  const [confirmPasswordVal, setConfirmPasswordVal] = useState(false);\n\n  const [value, setValue] = useState({\n    name: \"\",\n    lastName: \"\",\n    phone: \"\",\n    email: \"\",\n    password: \"\",\n    confirmPassword: \"\",\n  });\n\n  const [reveal, setReveal] = useState(false);\n\n  const onFinish = (values) => {\n    axios\n      .post(\"https://api-capstone-grupo04.herokuapp.com/users\", { ...values })\n      .then((res) => {\n        console.log(res);\n        history.push(\"/login\");\n      })\n      .catch((err) => console.log(err));\n  };\n\n  return (\n    <Box round>\n      <CardHeader>\n        <h2>Registre-se</h2>\n      </CardHeader>\n\n      <Box align=\"center\" justify=\"center\" pad=\"small\">\n        <Form\n          value={value}\n          validate=\"blur\"\n          onChange={(val) => setValue(val)}\n          onSubmit={({ value: val }) => onFinish(val)}\n        >\n          <Box direction=\"row\">\n            <FormField\n              label=\"Nome\"\n              name=\"name\"\n              icon={<User />}\n              component={TextInput}\n              required\n              validate={[\n                { regexp: /^[a-zA-Z]+$/i, message: \"Somente letras.\" },\n                (name) => {\n                  if (name.length >= 3) {\n                    setNameVal(true);\n                  }\n                  return undefined;\n                },\n              ]}\n            />\n            <Box align=\"center\" justify=\"center\">\n              {nameVal && <StatusGood />}\n            </Box>\n          </Box>\n\n          <Box direction=\"row\">\n            <FormField\n              label=\"Sobrenome\"\n              name=\"lastName\"\n              icon={<User />}\n              validate={[\n                { regexp: /^[a-zA-Z ]+$/i, message: \"Somente letras.\" },\n                (lastName) => {\n                  if (lastName.length >= 2) {\n                    setLastNameVal(true);\n                  }\n                  return undefined;\n                },\n              ]}\n            />\n            <Box align=\"center\" justify=\"center\">\n              {lastNameVal && <StatusGood />}\n            </Box>\n          </Box>\n\n          <Box direction=\"row\">\n            <FormField\n              label=\"Telefone\"\n              name=\"phone\"\n              icon={<Phone />}\n              validate={[\n                (phone) => {\n                  if (phone.length === 10) {\n                    setPhoneVal(true);\n                  }\n                  return undefined;\n                },\n              ]}\n            />\n            <Box align=\"center\" justify=\"center\">\n              {phoneVal && <StatusGood />}\n            </Box>\n          </Box>\n\n          <Box direction=\"row\">\n            <FormField\n              label=\"E-mail\"\n              name=\"email\"\n              icon={<MailOption />}\n              required\n              validate={[\n                {\n                  regexp: /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))+$/,\n                  placeholder: \"exemplo@ex.com\",\n                  message: \"E-mail inválido.\",\n                },\n                (email) => {\n                  if (email.length > 5) {\n                    setEmailVal(true);\n                  }\n                  return undefined;\n                },\n              ]}\n            />\n            <Box align=\"center\" justify=\"center\">\n              {emailVal && <StatusGood />}\n            </Box>\n          </Box>\n\n          <Box direction=\"row\">\n            <FormField\n              label=\"Senha\"\n              name=\"password\"\n              required\n              icon={<Lock />}\n              type={reveal ? \"text\" : \"password\"}\n              validate={[\n                (password) => {\n                  if (password.length > 2) {\n                    setPasswordVal(true);\n                  }\n                  return undefined;\n                },\n              ]}\n            />\n            <Button\n              icon={reveal ? <View size=\"medium\" /> : <Hide size=\"medium\" />}\n              onClick={() => setReveal(!reveal)}\n            />\n            <Box align=\"center\" justify=\"center\">\n              {passwordVal && <StatusGood />}\n            </Box>\n          </Box>\n          <Box direction=\"row\">\n            <FormField\n              label=\"Confirmar Senha\"\n              name=\"confirmPassword\"\n              required\n              icon={<Lock />}\n              type={reveal ? \"text\" : \"password\"}\n              validate={[\n                (confirmPassword) => {\n                  if (confirmPassword === value.password) {\n                    setConfirmPasswordVal(true);\n                    return;\n                  }\n                  return { message: \"As senhas não estão iguais!\" };\n                },\n              ]}\n            />\n            <Box align=\"center\" justify=\"center\">\n              {confirmPasswordVal && <StatusGood />}\n            </Box>\n          </Box>\n          <Box align=\"center\">\n            <Button primary label=\"Enviar\" type=\"submit\" />\n          </Box>\n        </Form>\n      </Box>\n    </Box>\n  );\n};\n\nexport default UserRegister;\n",["116","117"],"/home/renan/Documentos/Kenzie-Q2/entregas